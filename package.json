{
  "name": "pass-emploi-connect",
  "private": true,
  "version": "1.6.0",
  "license": "CEJ",
  "engines": {
    "node": "22.14.0"
  },
  "scripts": {
    "build": "nest build",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint:fix": "eslint --ext \".ts\" --fix --ignore-path .gitignore .",
    "start": "nest start",
    "watch": "nest start --watch | npx pino-pretty",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:redis:db": "docker compose up -d redis",
    "test:local": "yarn start:redis:db && REDIS_URL=redis://:myredispassword@redis:6379/0 mocha 'test/**/*.test.ts' --exit --timeout 10000",
    "cover": "nyc yarn test:local",
    "cover:report": "nyc report --reporter=lcovonly",
    "test": "yarn cover && yarn cover:report",
    "test:ci": "nyc --reporter=lcovonly mocha 'test/**/*.test.ts' --exit --timeout 10000",
    "generate-key-pair": "node scripts/generate-key-pair.mjs",
    "release:patch": "scripts/release-version.sh patch",
    "release:minor": "scripts/release-version.sh minor",
    "release:major": "scripts/release-version.sh major"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@types/express": "^5.0.0",
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "@types/oidc-provider": "^8.5.1",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/sanitize-html": "^2.11.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.4",
    "elastic-apm-node": "3.52.2",
    "ioredis": "^5.3.2",
    "joi": "^17.12.2",
    "jose": "^5.2.4",
    "lodash": "^4.17.21",
    "lodash.isempty": "^4.4.0",
    "luxon": "^3.4.4",
    "nestjs-pino": "^4.0.0",
    "oidc-provider": "^8.8.0",
    "openid-client": "^5.6.5",
    "pino-http": "^9.0.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.13.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.0",
    "@salesforce/ts-sinon": "^1.4.19",
    "@swc/cli": "^0.7.0",
    "@swc/core": "^1.4.0",
    "@types/chai-as-promised": "^7.1.8",
    "@types/dirty-chai": "^2.0.4",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.2",
    "chai-exclude": "^2.1.0",
    "dirty-chai": "^2.0.1",
    "dotvault": "^0.0.9",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-chai-friendly": "^0.7.4",
    "eslint-plugin-mocha": "^10.4.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^11.0.0",
    "nock": "^14.0.0",
    "nyc": "^17.0.0",
    "prettier": "^2.8.0",
    "sinon": "^20.0.0",
    "sinon-chai": "^3.7.0",
    "supertest": "^7.0.0"
  },
  "lint-staged": {
    "*.ts": "yarn lint:fix"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "packageManager": "yarn@4.9.1",
  "resolutions": {
    "cross-spawn": "7.0.6"
  }
}
